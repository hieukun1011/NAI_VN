<?xml version="1.0" encoding="UTF-8"?>

<templates>
    <t t-name="UpdatePartnerRequest.action_update_info_partner" owl="1">
        <button type="button" class="btn btn-secondary o_button_generate_leads" t-on-click="onClickUpdateInfo">
            Update
        </button>
        <button type="button" class="btn btn-secondary o_button_generate_leads" t-on-click="onClickClose">
            Close
        </button>
    </t>

    <t t-name="UpdatePartnerRequestListView.buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <!-- Before the export button -->
        <xpath expr="(//div/*)[last()]" position="after">
            <t t-call="UpdatePartnerRequest.action_update_info_partner"/>
        </xpath>
    </t>
</templates>
