<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="history_contact.PhoneFieldTree" owl="1">
        <div class="o_phone_content d-inline-flex w-100">
            <t t-if="props.readonly">
                <a t-if="props.value_data" class="o_form_uri" t-esc="props.value_data"/>
            </t>
        </div>
    </t>

    <t t-name="history_contact.FormPhoneField" t-inherit="web.FormPhoneField" t-inherit-mode="extension">
        <xpath expr="//input" position="after">
            <a
                t-if="props.value"
                t-att-title="title"
                href="#"
                t-on-click.prevent.stop="SearchprofilingHref"
                class="ms-3 d-inline-flex align-items-center"
            ><i class="fa fa-search"></i><small class="fw-bold ms-1">search</small></a>
        </xpath>
    </t>
</templates>
