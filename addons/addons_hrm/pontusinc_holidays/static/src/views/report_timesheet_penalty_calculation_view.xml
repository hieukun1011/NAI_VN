<?xml version="1.0" encoding="UTF-8"?>

<templates>
    <t t-name="ReportTimesheetPenaltyCalculation.action_report_timesheet_penalty_calculation" owl="1">
        <button type="button" class="btn btn-secondary o_button_generate_leads" t-on-click="onClickReport">
            Report
        </button>
    </t>

    <t t-name="ReportTimesheetPenaltyCalculationListView.buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
        <!-- Before the export button -->
        <xpath expr="(//div/*)[last()]" position="after">
            <t t-call="ReportTimesheetPenaltyCalculation.action_report_timesheet_penalty_calculation"/>
        </xpath>
    </t>
</templates>
