<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="customer_360_log_rank_view_form" model="ir.ui.view">
        <field name="name">Customer 360 log rank view form</field>
        <field name="model">partner.rank.history</field>
        <field name="priority" eval="1"/>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="partner_id" invisible="1"/>
                            <field name="rank_id" required="1"/>
                            <field name="level_id" required="1"/>
                        </group>
                        <group>
                            <field name="is_expected" invisible="1"/>
                            <field name="start" required="1"/>
                            <field name="stop"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>
